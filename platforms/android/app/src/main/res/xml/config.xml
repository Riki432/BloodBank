<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.bloodbank" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PhoneDialer">
        <param name="android-package" value="org.apache.cordova.phonedialer.PhoneDialer" />
    </feature>
    <feature name="DeviceFeedback">
        <param name="android-package" value="cz.Velda.cordova.plugin.devicefeedback.DeviceFeedback" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Sms">
        <param name="android-package" value="com.cordova.plugins.sms.Sms" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <name>Blood Bank</name>
    <description>
        An application developed to help people donate blood to those in need.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Sakshi Evelyn Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="www/assets/AppLogo.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
